{
    "name": "mu/blogging-module",
    "version": "1.0.0",
    "description": "MUBloggingModule module generated by ModuleStudio 0.7.4.",
    "type": "zikula-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Michael Ueberschaer",
            "email": "info@homepages-mit-zikula.de",
            "homepage": "http://homepages-mit-zikula.de",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "MU\\BloggingModule\\": "" }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "suggest": {
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=1.5.0 <3.0",
            "class": "MU\\BloggingModule\\MUBloggingModule",
            "displayname": "Blogging",
            "url": "blogging",
            "capabilities": {
                "user": {"route": "mubloggingmodule_post_index"},
                "admin": {"route": "mubloggingmodule_post_adminindex"},
                "categorizable": {
                    "entities": [
                        "MU\\BloggingModule\\Entity\\PostEntity"
                    ]
                },
                "hook_subscriber": {"class": "MU\\BloggingModule\\Container\\HookContainer"}
            },
            "securityschema": {
                "MUBloggingModule::": "::",
                "MUBloggingModule:ItemListBlock:": "Block title::",
                "MUBloggingModule:ModerationBlock:": "Block title::",
                "MUBloggingModule:Post:": "Post ID::",
                "MUBloggingModule:Post:Post": "Post ID:Post ID:",
                "MUBloggingModule::Ajax": "::"
            }
        }
    }
}
